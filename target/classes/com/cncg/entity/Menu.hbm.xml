<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cncg.entity">

	<class name="Menu" table="t_menu" dynamic-update="true" dynamic-insert="true">
		<id name="menuId" column="menuId">
			<generator class="native"></generator>
		</id>
		
		<property name="menuName"></property>
		<property name="menuPath"></property>
		<property name="parentId"></property>
		<property name="menuDescription"></property>
		<property name="state"></property>
		<property name="iconCls"></property>
	</class>

</hibernate-mapping>